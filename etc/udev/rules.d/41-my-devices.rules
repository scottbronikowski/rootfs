# udev rules to give fixed symlinks for devices

#USB GPS antenna (**OUPTUTS DATA AT 4800 bps)
SUBSYSTEM=="tty", ATTRS{idVendor}=="067b", ATTRS{idProduct}=="2303", SYMLINK+="GPS", MODE="0666"

#USB adapter attached to Razor IMU
SUBSYSTEM=="tty", ATTRS{idVendor}=="0403", ATTRS{idProduct}=="6001", SYMLINK+="RazorIMU", MODE="0666"

#Teensy board (monitors encoders)
SUBSYSTEM=="tty", ATTRS{idVendor}=="16c0", ATTRS{idProduct}=="04[789]?", SYMLINK+="Teensy", MODE="0666"

#end