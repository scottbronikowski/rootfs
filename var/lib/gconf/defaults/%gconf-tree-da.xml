<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="gnumeric">
				<dir name="plugin">
					<dir name="latex">
						<entry name="use-utf8">
							<local_schema short_desc="Brug UTF-8 i LaTeX-eksport">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="core">
					<dir name="sort">
						<dir name="default">
							<entry name="ascending">
								<local_schema short_desc="Sortér stigende">
								</local_schema>
							</entry>
							<entry name="by-case">
								<local_schema short_desc="Skeln mellem store og små bogstaver">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="gui">
						<dir name="editing">
							<entry name="autocomplete">
								<local_schema short_desc="Autofuldførelse">
								</local_schema>
							</entry>
						</dir>
						<dir name="screen">
							<entry name="verticaldpi">
								<local_schema short_desc="Lodret DPI">
								</local_schema>
							</entry>
						</dir>
					</dir>
				</dir>
				<dir name="searchreplace">
					<entry name="regex">
						<local_schema short_desc="Søgetype for søg og erstat">
						</local_schema>
					</entry>
					<entry name="scope">
						<local_schema short_desc="Virkefelt for søg og erstat">
							<longdesc>Dette er standardvirkefeltet for søg og erstat. 0: hele arbejdsbogen; 1: aktuelt ark; 2: interval</longdesc>
						</local_schema>
					</entry>
					<entry name="whole-words-only">
						<local_schema short_desc="Kun hele ord for søg og erstat">
						</local_schema>
					</entry>
				</dir>
				<dir name="stf">
					<dir name="export">
						<entry name="separator">
							<local_schema short_desc="Feltskilletegn for teksteksport">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="printsetup">
					<entry name="preferred-unit">
						<local_schema>
							<longdesc>Denne streng angiver standardenheden, der skal bruges i sideopsætningsdialogen.</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="autoformat">
					<entry name="usr-dir">
						<local_schema short_desc="Brugermappe for autoformater">
							<longdesc>Hovedmappen for brugerspecifikke autoformatskabeloner.</longdesc>
						</local_schema>
					</entry>
					<entry name="sys-dir">
						<local_schema short_desc="Systemmappe for autoformater">
							<longdesc>Denne mappe indeholder de præinstallerede autoformatskabeloner.</longdesc>
						</local_schema>
					</entry>
					<entry name="extra-dirs">
						<local_schema short_desc="Liste af ekstra autoformatmapper.">
							<longdesc>Denne liste indeholder alle ekstra mapper, der indeholder autoformatskabeloner.</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="plugins">
					<entry name="extra-dirs">
						<local_schema>
							<longdesc>Denne liste indeholder alle ekstra mapper, der indeholder udvidelsesmoduler.</longdesc>
						</local_schema>
					</entry>
					<entry name="known">
						<local_schema>
							<longdesc>Denne liste indeholder alle kendte udvidelsesmoduler.</longdesc>
						</local_schema>
					</entry>
					<entry name="active">
						<local_schema short_desc="Liste af aktive udvidelsesmoduler.">
							<longdesc>Denne liste indeholder alle udvidelsesmoduler, som skal aktiveres automatisk.</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="autocorrect">
					<entry name="replace">
						<local_schema short_desc="Automatiske rettelser">
							<longdesc>Automatiske rettelser</longdesc>
						</local_schema>
					</entry>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
