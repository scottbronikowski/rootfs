<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="gnome-settings">
				<dir name="gnumeric">
					<entry name="toolbar_style">
						<local_schema short_desc="Styl paska narzędziowego">
							<longdesc>Styl paska narzędziowego. Poprawnymi wartościami są both, both_horiz, icon i text</longdesc>
						</local_schema>
					</entry>
				</dir>
			</dir>
			<dir name="gnumeric">
				<dir name="core">
					<dir name="sort">
						<dir name="default">
							<entry name="ascending">
								<local_schema short_desc="Rosnąco">
									<longdesc>Ta opcja określa początkowy stan przycisku porządku sortowania w oknie dialogowym sortowania.</longdesc>
								</local_schema>
							</entry>
							<entry name="retain-formats">
								<local_schema short_desc="Sortowanie zachowuje formaty">
								</local_schema>
							</entry>
							<entry name="by-case">
								<local_schema short_desc="Sortowanie z _rozróżnianiem małych i wielkich liter">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="file">
						<dir name="save">
							<entry name="def-overwrite">
								<local_schema short_desc="Domyślne nadpisywanie plików">
									<longdesc>Zanim istniejący plik zostanie nadpisany, gnumeric wyświetli okno z ostrzeżeniem. Ustawienie tej opcji uczyni przycisk nadpisywania w tym oknie dialogowym przyciskiem domyślnym.</longdesc>
								</local_schema>
							</entry>
							<entry name="single_sheet">
								<local_schema short_desc="Ostrzeganie przed eksportem do formatu pojedynczego arkusza">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="xml">
						<entry name="compression-level">
							<local_schema short_desc="Domyślny poziom kompresji dla plików Gnumerica">
							</local_schema>
						</entry>
					</dir>
					<dir name="gui">
						<dir name="window">
							<entry name="zoom">
								<local_schema short_desc="Domyślny współczynnik powiększenia">
									<longdesc>Początkowy współczynnik powiększenia dla nowych zeszytów.</longdesc>
								</local_schema>
							</entry>
							<entry name="y">
								<local_schema short_desc="Domyślny rozmiar okna w pionie">
									<longdesc>Ta liczba (pomiędzy 0,25 a 1,00) określa część rozmiaru ekranu zajmowaną przez domyślne okno w pionie.</longdesc>
								</local_schema>
							</entry>
							<entry name="x">
								<local_schema short_desc="Domyślny rozmiar okna w poziomie">
									<longdesc>Ta liczba (pomiędzy 0,25 a 1,00) określa część rozmiaru ekranu zajmowaną przez domyślne okno w poziomie.</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="editing">
							<entry name="recalclag">
								<local_schema short_desc="Opóźnienie automatycznego przeliczania wyrażenia">
								</local_schema>
							</entry>
							<entry name="livescrolling">
								<local_schema short_desc="Przewijanie na bieżąco">
								</local_schema>
							</entry>
							<entry name="autocomplete">
								<local_schema short_desc="Automatyczne uzupełnianie">
									<longdesc>Ta zmienna określa czy ustawione jest automatyczne uzupełnianie.</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="screen">
							<entry name="verticaldpi">
								<local_schema short_desc="Rozdzielczość DPI w pionie">
									<longdesc>Rozdzielczość ekranu w pionie.</longdesc>
								</local_schema>
							</entry>
							<entry name="horizontaldpi">
								<local_schema short_desc="Rozdzielczość DPI w poziomie">
									<longdesc>Rozdzielczość ekranu w poziomie.</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="workbook">
						<entry name="n-sheet">
							<local_schema short_desc="Domyślna liczba arkuszy">
								<longdesc>Liczba arkuszy początkowo tworzonych w nowym zeszycie.</longdesc>
							</local_schema>
						</entry>
					</dir>
					<dir name="defaultfont">
						<entry name="italic">
							<local_schema short_desc="Domyślna czcionka jest pochylona.">
							</local_schema>
						</entry>
						<entry name="bold">
							<local_schema short_desc="Domyślna czcionka jest pogrubiona.">
								<longdesc>Ta wartość określa czy domyślna czcionka dla nowych zeszytów jest wytłuszczona.</longdesc>
							</local_schema>
						</entry>
						<entry name="name">
							<local_schema short_desc="Domyślna nazwa czcionki">
								<longdesc>Domyślna nazwa czcionki dla nowych zeszytów.</longdesc>
							</local_schema>
						</entry>
						<entry name="size">
							<local_schema short_desc="Domyślny rozmiar czcionki">
								<longdesc>Domyślny rozmiar czcionki dla nowych zeszytów.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="undo">
					<entry name="max_descriptor_width">
						<local_schema short_desc="Długość deskryptorów cofnięcia">
						</local_schema>
					</entry>
					<entry name="size">
						<local_schema short_desc="Maksymalna liczba cofnięć">
						</local_schema>
					</entry>
					<entry name="show_sheet_name">
						<local_schema short_desc="Wyświetlanie nazwy arkusza na liście cofnięć">
						</local_schema>
					</entry>
				</dir>
				<dir name="printsetup">
					<entry name="footer">
						<local_schema short_desc="Stopka strony">
						</local_schema>
					</entry>
					<entry name="header">
						<local_schema short_desc="Nagłówek strony">
						</local_schema>
					</entry>
				</dir>
				<dir name="autoformat">
					<entry name="usr-dir">
						<local_schema short_desc="Katalog użytkownika na automatyczne formaty">
							<longdesc>Główny katalog na szablony automatycznego formatowania specyficzne dla użytkownika.</longdesc>
						</local_schema>
					</entry>
					<entry name="sys-dir">
						<local_schema>
							<longdesc>Ten katalog zawiera przeinstalowane szablony automatycznego formatowania.</longdesc>
						</local_schema>
					</entry>
					<entry name="extra-dirs">
						<local_schema>
							<longdesc>Ta lista zawiera wszystkie dodatkowe katalogi zawierające szablony automatycznego formatowania.</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="plugins">
					<entry name="extra-dirs">
						<local_schema short_desc="Lista dodatkowych katalogów z wtyczkami.">
							<longdesc>Ta lista zawiera wszystkie dodatkowe katalogi zawierające wtyczki.</longdesc>
						</local_schema>
					</entry>
					<entry name="file-states">
						<local_schema short_desc="Lista stanów plików z wtyczkami.">
							<longdesc>Ta lista zawiera wszystkie stany plików z wtyczkami.</longdesc>
						</local_schema>
					</entry>
					<entry name="known">
						<local_schema short_desc="Lista znanych wtyczek.">
							<longdesc>Ta lista zawiera wszystkie znane wtyczki.</longdesc>
						</local_schema>
					</entry>
					<entry name="active">
						<local_schema short_desc="Lista aktywnych wtyczek.">
							<longdesc>Ta lista zwiera wszystkie wtyczki, które powinny być automatycznie aktywowane.</longdesc>
						</local_schema>
					</entry>
					<entry name="activate-new">
						<local_schema>
							<longdesc>Ta zmienna określa czy należy aktywować każdą nowonapotkaną wtyczkę.</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="functionselector">
					<entry name="num-of-recent">
						<local_schema short_desc="Maksymalna długość listy ostatnio używanych funkcji">
						</local_schema>
					</entry>
					<entry name="recentfunctions">
						<local_schema short_desc="Lista ostatnio używanych funkcji.">
						</local_schema>
					</entry>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
