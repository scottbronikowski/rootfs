<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="gnumeric">
				<dir name="core">
					<dir name="file">
						<dir name="save">
							<entry name="def-overwrite">
								<local_schema short_desc="Стандардно за запишување над датотеки">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="xml">
						<entry name="compression-level">
							<local_schema short_desc="Стандардно ниво на компресија за Gnumeric датотеките">
							</local_schema>
						</entry>
					</dir>
					<dir name="gui">
						<dir name="window">
							<entry name="zoom">
								<local_schema short_desc="Стандарден фактор на зумирање">
								</local_schema>
							</entry>
							<entry name="y">
								<local_schema short_desc="Стандардна вертикална големина на прозорец">
								</local_schema>
							</entry>
							<entry name="x">
								<local_schema short_desc="Стандардна хоризонтална големина на прозорец">
								</local_schema>
							</entry>
						</dir>
						<dir name="editing">
							<entry name="autocomplete">
								<local_schema short_desc="Автоматско довршување">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="workbook">
						<entry name="n-sheet">
							<local_schema short_desc="Стандарден број на листови">
							</local_schema>
						</entry>
					</dir>
					<dir name="defaultfont">
						<entry name="italic">
							<local_schema short_desc="Стандардниот фонт е курзив.">
							</local_schema>
						</entry>
						<entry name="bold">
							<local_schema short_desc="Стандардниот фонт е задебелен.">
							</local_schema>
						</entry>
						<entry name="name">
							<local_schema short_desc="Име на стандардниот фонт">
								<longdesc>Стандардното име на фонтот за новите работни книги.</longdesc>
							</local_schema>
						</entry>
						<entry name="size">
							<local_schema short_desc="Стандардна големина на фонтот">
								<longdesc>Стандардната големина на фонтовите за новите работни книги.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="undo">
					<entry name="max_descriptor_width">
						<local_schema short_desc="Должина на опишувачите за отповикување">
						</local_schema>
					</entry>
					<entry name="maxnum">
						<local_schema short_desc="Број на отповикливи предмети">
						</local_schema>
					</entry>
					<entry name="size">
						<local_schema short_desc="Максимална големина на отповикувања">
						</local_schema>
					</entry>
					<entry name="show_sheet_name">
						<local_schema short_desc="Прикажи го името на листот во листата за отповикување">
						</local_schema>
					</entry>
				</dir>
				<dir name="dialogs">
					<dir name="rs">
						<entry name="unfocused">
							<local_schema short_desc="Дозволи избор на нефокусиран опсег">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="printsetup">
					<entry name="print-titles">
						<local_schema short_desc="Стандарден наслов за печатење">
						</local_schema>
					</entry>
					<entry name="footer">
						<local_schema short_desc="Подножје на страницата">
						</local_schema>
					</entry>
					<entry name="header">
						<local_schema short_desc="Заглавие на страницата">
						</local_schema>
					</entry>
				</dir>
				<dir name="functionselector">
					<entry name="num-of-recent">
						<local_schema short_desc="Максимална должина на листата на скоро користени функции">
						</local_schema>
					</entry>
				</dir>
				<dir name="plugin">
					<dir name="latex">
						<entry name="use-utf8">
							<local_schema short_desc="Користи UTF-8 подршка при извозот во LaTeX">
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
