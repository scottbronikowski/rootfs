<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="gnome-settings">
				<dir name="gnumeric">
					<entry name="toolbar_style">
						<local_schema short_desc="Araç Çubuğu Tarzı">
							<longdesc>Araç çubuğunun yerleşimi. Geçerli değerler, icon (simge), text (metin), both (herikisi altalta) ve both_horiz (herikisi yanyana)</longdesc>
						</local_schema>
					</entry>
				</dir>
			</dir>
			<dir name="gnumeric">
				<dir name="core">
					<dir name="sort">
						<dir name="dialog">
							<entry name="max-initial-clauses">
								<local_schema short_desc="Otomatik Eklenen Sözcüklerin Sayısı">
								</local_schema>
							</entry>
						</dir>
						<dir name="default">
							<entry name="ascending">
								<local_schema short_desc="Artan Sıralama">
									<longdesc>Bu seçenek, sıralama diyaloğundaki sırala düğmesinin durumunu belirler.</longdesc>
								</local_schema>
							</entry>
							<entry name="retain-formats">
								<local_schema>
									<longdesc>Bu seçenek etkin olduğunda araç çubuğunda sıralama sırasında göze biçemlerini saklayacak sıralama düğmeleri etkin olur ve sıralama diyaloğundaki biçemleri saklama onay kutularının durumunu belirler.</longdesc>
								</local_schema>
							</entry>
							<entry name="by-case">
								<local_schema short_desc="Sıralama Harf Büyüklüğüne Duyarlıdır">
									<longdesc>Bu seçenek etkin olduğunda araç çubuğunda harf büyüklüğüne göre sıralama düğmeleri etkin olur ve sıralama diyaloğundaki harf büyüklüğüne göre sıralama onay kutularının durumunu belirler.</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="file">
						<dir name="save">
							<entry name="def-overwrite">
								<local_schema short_desc="Öntanımlı Üzerine Yazılacak Dosyalar">
									<longdesc>Mevcut bir dosyanın üzerine yazılmadan önce, Gnumeric bir uyarı verir. Bu seçenekle uyarı penceresindeki Üzerine Yaz düğmesi öntanımlı seçilen düğme olur.</longdesc>
								</local_schema>
							</entry>
							<entry name="single_sheet">
								<local_schema short_desc="Tek Çizelgelik Biçeme Çıktılandığında Uyar">
									<longdesc>Bazı dosya biçemleri tek bir çizelge içerir. Bu değişken çok çizelgeli bir defterin sadece bir çizelgesi kaydedileceği zaman kullanıcının uyarılıp uyarılmayacağını belirler.</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="xml">
						<entry name="compression-level">
							<local_schema short_desc="Gnumiric Dosyalarının Öntanımlı Sıkıştırma Seviyesi">
							</local_schema>
						</entry>
					</dir>
					<dir name="gui">
						<dir name="window">
							<entry name="zoom">
								<local_schema short_desc="Öntanımlı Büyütme Çarpanı">
									<longdesc>Yeni bir deferin hangi büyütme çarpanı ile açılacağını belirler.</longdesc>
								</local_schema>
							</entry>
							<entry name="y">
								<local_schema short_desc="Öntanımlı Pencere Düşey Uzunluğu">
									<longdesc>Bu sayı (0.25 ile 1.00 arasındadır) öntanımlı pencerenin ekrana düşey oranıdır.</longdesc>
								</local_schema>
							</entry>
							<entry name="x">
								<local_schema short_desc="Öntanımlı Pencere Yatay Uzunluğu">
									<longdesc>Bu sayı (0.25 ile 1.00 arasındadır) öntanımlı pencerenin ekrana yatay oranıdır.</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="editing">
							<entry name="recalclag">
								<local_schema short_desc="Otomatik Yeniden Hesaplama Gecikmesi">
								</local_schema>
							</entry>
							<entry name="livescrolling">
								<local_schema short_desc="Anında Kaydırma">
									<longdesc>Bu değişken kaydırmanın gecikmeli olup olmadığını belirler.</longdesc>
								</local_schema>
							</entry>
							<entry name="autocomplete">
								<local_schema short_desc="Otomatik Tamamla">
									<longdesc>Bu değişken otomatik tamamlamanın geçerli olup olmadığını belirler.</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="screen">
							<entry name="verticaldpi">
								<local_schema short_desc="Düşey DPI">
									<longdesc>Düşey ekran çözünürlüğü</longdesc>
								</local_schema>
							</entry>
							<entry name="horizontaldpi">
								<local_schema short_desc="Yatay DPI">
									<longdesc>Yatay ekran çözünürlüğü</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="workbook">
						<entry name="n-sheet">
							<local_schema short_desc="Öntanımlı Çizelge Sayısı">
								<longdesc>Yeni bir defterin kaç çizelge ile açılacağını belirler.</longdesc>
							</local_schema>
						</entry>
					</dir>
					<dir name="defaultfont">
						<entry name="italic">
							<local_schema short_desc="Öntanımlı yazıtipi eğiktir.">
							</local_schema>
						</entry>
						<entry name="bold">
							<local_schema short_desc="Öntanımlı yazıtipi kalındır.">
							</local_schema>
						</entry>
						<entry name="name">
							<local_schema short_desc="Öntanımlı yazıtipi ismi">
								<longdesc>Yeni bir defterin öntanımlı yazıtipi ismi.</longdesc>
							</local_schema>
						</entry>
						<entry name="size">
							<local_schema short_desc="Öntanımlı Yazıtipi Yüksekliği">
								<longdesc>Yeni bir defterin öntanımlı yazıtipi boyu.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="undo">
					<entry name="max_descriptor_width">
						<local_schema short_desc="Geri Al Açıklamalarının Uzunluğu">
							<longdesc>Bu değer, Geri Al ve Tekrar Yap zincirlerindeki azami kayıtlı eylem sayısıdır.</longdesc>
						</local_schema>
					</entry>
					<entry name="maxnum">
						<local_schema short_desc="Geri Al Öğelerinin Sayısı">
							<longdesc>Bu değer, Geri Al/Yeniden Yap listelerindeki azami öğe sayısını belirler.</longdesc>
						</local_schema>
					</entry>
					<entry name="size">
						<local_schema short_desc="Azami Geri Alma Sayısı">
							<longdesc>Bu değer, Geri Al zincirinin uzunluğunu belirler. Her düzenleme eylemi, bu değerle ilişkili uzunluktadır. Bu ilişki, bir basit gözenin düzenlenmesinin (bu eylemin değeri 1 dir) bellek gereksinimine göredir. Geri Al listesi, bu yapılandırma değerini aşan toplam bellekle kırpılır.</longdesc>
						</local_schema>
					</entry>
					<entry name="show_sheet_name">
						<local_schema short_desc="Geri Al Listesinde Çizelge İsmini Göster">
							<longdesc>Bu değer, Geri Al ve Tekrar Yap listelerinde çizelge isimlerinin gösterilip gösterilmeyeceğini belirler.</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="dialogs">
					<dir name="rs">
						<entry name="unfocused">
							<local_schema short_desc="Odaksız Aralık Seçimine İzin Ver">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="printsetup">
					<entry name="all-sheets">
						<local_schema short_desc="Tüm çizelgelere yazdırma ayarını uygular">
							<longdesc>Bu değer, yazıcı ayarları penceresinin tüm çizelgelere öntanımlı olarak aynı anda uygulanıp uygulanmayacağını belirler.</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="autoformat">
					<entry name="usr-dir">
						<local_schema short_desc="Otomatik Biçemler için Kullanıcı Dizini">
							<longdesc>Kullanıcıya özel otomatik biçemleme şablonlarının ana dizini.</longdesc>
						</local_schema>
					</entry>
					<entry name="sys-dir">
						<local_schema short_desc="Otomatik Biçemleme için Sistem Dizini">
							<longdesc>Bu dizin gnumeric ile gelen otomatik biçemleme şablonlarını içerir.</longdesc>
						</local_schema>
					</entry>
					<entry name="extra-dirs">
						<local_schema short_desc="İlave Otomatik Biçemleme Dizinleri Listesi">
							<longdesc>Bu liste otomatik biçemleme şablonlarının bulunduğu dizinleri içerir.</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="plugins">
					<entry name="extra-dirs">
						<local_schema short_desc="İlave Eklenti Dizinleri Listesi.">
							<longdesc>Bu liste, eklentileri içeren dizinleri içerir.</longdesc>
						</local_schema>
					</entry>
					<entry name="file-states">
						<local_schema short_desc="Eklenti Dosyalarının Durum Listesi.">
							<longdesc>Bu liste eklenti dosya durumlarını içerir.</longdesc>
						</local_schema>
					</entry>
					<entry name="known">
						<local_schema short_desc="Bilinen Eklentiler Listesi.">
							<longdesc>Bu liste bilinen eklentileri içerir.</longdesc>
						</local_schema>
					</entry>
					<entry name="active">
						<local_schema short_desc="Etkin Eklentilerin Listesi.">
							<longdesc>Bu liste, otomatik olarak etkinleştirileceği varsayılan eklentileri içerir.</longdesc>
						</local_schema>
					</entry>
					<entry name="activate-new">
						<local_schema short_desc="Yeni Eklentileri Etkinleştir">
							<longdesc>Bu değişken, yeni bir eklenti saptandığında eklentinin otomatik olarak yüklenip yüklenmeyeceğini belirler.</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="autocorrect">
					<entry name="replace">
						<local_schema short_desc="Otomatik düzelterek değiştirir">
							<longdesc>Otomatik düzelterek değiştirir</longdesc>
						</local_schema>
					</entry>
					<entry name="names-of-days">
						<local_schema short_desc="Gün isimlerini otomatik düzeltir">
							<longdesc>Gün isimlerini otomatik düzeltir</longdesc>
						</local_schema>
					</entry>
					<entry name="first-letter-list">
						<local_schema short_desc="İlk Harfi Düzeltilmeyecekler Listesi">
							<longdesc>Otomatik düzeltme motoru bu listedeki sözcüklerin ilk harflerini düzeltmez.</longdesc>
						</local_schema>
					</entry>
					<entry name="first-letter">
						<local_schema short_desc="İlk harfleri otomatik düzeltir">
							<longdesc>İlk harfleri otomatik düzeltir</longdesc>
						</local_schema>
					</entry>
					<entry name="init-caps-list">
						<local_schema short_desc="Baş Harfi Düzeltilmeyecekler Listesi">
						</local_schema>
					</entry>
					<entry name="init-caps">
						<local_schema short_desc="Baş harfleri otomatik düzeltir">
							<longdesc>Baş harfleri otomatik düzeltir</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="functionselector">
					<entry name="num-of-recent">
						<local_schema short_desc="Son Kullanılan İşlevler Listesinin Azami Uzunluğu">
							<longdesc>İşlev seçici son kullanılan işlevlerin listesini tutar. Bu değer, bu listenin azami uzunluğunu belirler.</longdesc>
						</local_schema>
					</entry>
					<entry name="recentfunctions">
						<local_schema short_desc="Son kullanılan işlevler listesi.">
							<longdesc>İşlev seçici son kullanılan işlevlerin listesini tutar. Bu o listedir.</longdesc>
						</local_schema>
					</entry>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
