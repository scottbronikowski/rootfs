<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="gnome-settings">
				<dir name="gnumeric">
					<entry name="toolbar_style">
						<local_schema short_desc="工具栏样式">
							<longdesc>工具栏样式。有效值为二者、二者垂直、图标和文字</longdesc>
						</local_schema>
					</entry>
				</dir>
			</dir>
			<dir name="gnumeric">
				<dir name="core">
					<dir name="sort">
						<dir name="dialog">
							<entry name="max-initial-clauses">
								<local_schema short_desc="自动语句的数目">
								</local_schema>
							</entry>
						</dir>
						<dir name="default">
							<entry name="ascending">
								<local_schema short_desc="升序排序">
								</local_schema>
							</entry>
							<entry name="by-case">
								<local_schema short_desc="排序区分大小写">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="file">
						<dir name="save">
							<entry name="def-overwrite">
								<local_schema short_desc="默认覆盖文件">
								</local_schema>
							</entry>
							<entry name="single_sheet">
								<local_schema>
									<longdesc>某些文件格式只包含一张工作表。此变量决定了只保存有多张工作表的工作簿时是否要警告用户。</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="xml">
						<entry name="compression-level">
							<local_schema short_desc="Gnumeric 文件的默认压缩级别">
							</local_schema>
						</entry>
					</dir>
					<dir name="gui">
						<dir name="window">
							<entry name="zoom">
								<local_schema short_desc="默认缩放比例">
								</local_schema>
							</entry>
							<entry name="y">
								<local_schema short_desc="默认垂直窗口大小">
								</local_schema>
							</entry>
							<entry name="x">
								<local_schema short_desc="默认水平窗口大小">
								</local_schema>
							</entry>
						</dir>
						<dir name="editing">
							<entry name="livescrolling">
								<local_schema short_desc="实时滚动">
								</local_schema>
							</entry>
							<entry name="autocomplete">
								<local_schema short_desc="自动完成">
								</local_schema>
							</entry>
						</dir>
						<dir name="screen">
							<entry name="verticaldpi">
								<local_schema short_desc="垂直 DPI">
									<longdesc>垂直方向的屏幕分辨率。</longdesc>
								</local_schema>
							</entry>
							<entry name="horizontaldpi">
								<local_schema short_desc="水平 DPI">
									<longdesc>水平方向的屏幕分辨率。</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="workbook">
						<entry name="n-sheet">
							<local_schema short_desc="工作表的默认数目">
							</local_schema>
						</entry>
					</dir>
					<dir name="defaultfont">
						<entry name="italic">
							<local_schema short_desc="默认字体为斜体。">
							</local_schema>
						</entry>
						<entry name="bold">
							<local_schema short_desc="默认字体为粗体。">
							</local_schema>
						</entry>
						<entry name="name">
							<local_schema short_desc="默认字体名">
							</local_schema>
						</entry>
						<entry name="size">
							<local_schema short_desc="默认字体大小">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="undo">
					<entry name="maxnum">
						<local_schema short_desc="撤消项目的数目">
						</local_schema>
					</entry>
					<entry name="size">
						<local_schema short_desc="最大的撤消次数">
						</local_schema>
					</entry>
					<entry name="show_sheet_name">
						<local_schema short_desc="在撤消列表中显示工作表名">
						</local_schema>
					</entry>
				</dir>
				<dir name="printsetup">
					<entry name="across-then-down">
						<local_schema short_desc="默认打印方向">
						</local_schema>
					</entry>
					<entry name="footer">
						<local_schema short_desc="页脚">
						</local_schema>
					</entry>
					<entry name="header">
						<local_schema short_desc="页眉">
						</local_schema>
					</entry>
					<entry name="all-sheets">
						<local_schema short_desc="将打印设置应用到所有工作表">
						</local_schema>
					</entry>
				</dir>
				<dir name="autoformat">
					<entry name="usr-dir">
						<local_schema short_desc="使用目录自动套用格式">
						</local_schema>
					</entry>
					<entry name="sys-dir">
						<local_schema short_desc="自动套用格式的系统目录">
						</local_schema>
					</entry>
					<entry name="extra-dirs">
						<local_schema short_desc="额外的自动套用格式目录列表。">
						</local_schema>
					</entry>
				</dir>
				<dir name="plugins">
					<entry name="extra-dirs">
						<local_schema short_desc="额外的插件目录列表。">
						</local_schema>
					</entry>
					<entry name="file-states">
						<local_schema short_desc="插件文件状态的列表。">
						</local_schema>
					</entry>
					<entry name="known">
						<local_schema short_desc="已知插件的列表。">
						</local_schema>
					</entry>
					<entry name="active">
						<local_schema short_desc="激活插件列表。">
						</local_schema>
					</entry>
					<entry name="activate-new">
						<local_schema short_desc="激活新插件">
						</local_schema>
					</entry>
				</dir>
				<dir name="autocorrect">
					<entry name="replace">
						<local_schema short_desc="自动更正替换">
							<longdesc>自动更正替换</longdesc>
						</local_schema>
					</entry>
					<entry name="names-of-days">
						<local_schema short_desc="自动更正星期的名字">
							<longdesc>自动更正星期的名字</longdesc>
						</local_schema>
					</entry>
					<entry name="first-letter">
						<local_schema short_desc="自动更正首字母">
							<longdesc>自动更正首字母</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="functionselector">
					<entry name="recentfunctions">
						<local_schema short_desc="最近使用的函数列表。">
						</local_schema>
					</entry>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
