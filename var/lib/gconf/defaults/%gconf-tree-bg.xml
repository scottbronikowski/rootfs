						<entry name="n-sheet">
							<local_schema short_desc="Стандартен брой листове">
							</local_schema>
						</entry>
					</dir>
					<dir name="defaultfont">
						<entry name="italic">
							<local_schema short_desc="Стандартният шрифт е курсив.">
							</local_schema>
						</entry>
						<entry name="name">
							<local_schema short_desc="Стандартно име на шрифт">
