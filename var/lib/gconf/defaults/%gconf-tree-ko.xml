<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="gnome-settings">
				<dir name="gnumeric">
					<entry name="toolbar_style">
						<local_schema short_desc="도구 모음 모양">
						</local_schema>
					</entry>
				</dir>
			</dir>
			<dir name="gnumeric">
				<dir name="core">
					<dir name="sort">
						<dir name="default">
							<entry name="ascending">
								<local_schema short_desc="오름차순으로 정렬">
								</local_schema>
							</entry>
							<entry name="by-case">
								<local_schema short_desc="대소문자 구분">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="gui">
						<dir name="editing">
							<entry name="autocomplete">
								<local_schema short_desc="자동완성">
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="workbook">
						<entry name="n-sheet">
							<local_schema short_desc="기본 시트 개수">
							</local_schema>
						</entry>
					</dir>
					<dir name="defaultfont">
						<entry name="italic">
							<local_schema short_desc="글꼴을 기울입니다">
							</local_schema>
						</entry>
						<entry name="bold">
							<local_schema short_desc="글꼴을 굵게 합니다">
							</local_schema>
						</entry>
						<entry name="name">
							<local_schema short_desc="기본 글꼴 이름">
							</local_schema>
						</entry>
						<entry name="size">
							<local_schema short_desc="기본 글꼴 크기">
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="printsetup">
					<entry name="margin-gtk-right">
						<local_schema short_desc="기본 아래 여백">
						</local_schema>
					</entry>
					<entry name="margin-bottom">
						<local_schema short_desc="기본 아래 여백">
						</local_schema>
					</entry>
					<entry name="margin-top">
						<local_schema short_desc="기본 위 여백">
						</local_schema>
					</entry>
					<entry name="across-then-down">
						<local_schema short_desc="기본 인쇄 방향">
						</local_schema>
					</entry>
					<entry name="hf-font-italic">
						<local_schema short_desc="머리말/꼬리말에 글꼴을 기울입니다">
						</local_schema>
					</entry>
					<entry name="hf-font-bold">
						<local_schema short_desc="머리말/꼬리말에 글꼴을 굵게 합니다">
						</local_schema>
					</entry>
					<entry name="hf-font-name">
						<local_schema short_desc="기본 머리말/꼬리말 글꼴 이름">
							<longdesc>머리말/꼬리말을 위한 기본 글꼴 이름</longdesc>
						</local_schema>
					</entry>
					<entry name="hf-font-size">
						<local_schema short_desc="기본 머리말/꼬리말 글꼴 크기">
							<longdesc>머리말/꼬리말을 위한 기본 글꼴 크기</longdesc>
						</local_schema>
					</entry>
					<entry name="footer">
						<local_schema short_desc="페이지 꼬리말">
						</local_schema>
					</entry>
					<entry name="header">
						<local_schema short_desc="페이지 머리말">
						</local_schema>
					</entry>
				</dir>
				<dir name="autocorrect">
					<entry name="names-of-days">
						<local_schema short_desc="날짜의 이름을 대문자로 바꾸기">
							<longdesc>날짜의 이름을 대문자로 바꾸기</longdesc>
						</local_schema>
					</entry>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
