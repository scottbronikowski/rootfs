#!/bin/sh

set -e

action="$1"

# remove symlink in symlink created in upgrades on systems which had a version
# prior to 1:2.6.0-2
if [ upgrade = "$action" ]; then
    version="$2"
    if dpkg --compare-versions 1:2.6.0-4 ge-nl "$version"; then
        rm -f /usr/share/doc/libglade2-dev/html/libglade
    fi

fi


