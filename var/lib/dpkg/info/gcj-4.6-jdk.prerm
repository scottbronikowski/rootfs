#! /bin/sh -e

if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ]; then
    update-alternatives --quiet --remove javac /usr/bin/gcj-wrapper-4.6
    update-alternatives --quiet --remove jar /usr/bin/gjar-4.6
    update-alternatives --quiet --remove jarsigner /usr/bin/gjarsigner-4.6
    update-alternatives --quiet --remove javah /usr/bin/gjavah-4.6
    update-alternatives --quiet --remove javadoc /usr/bin/gjdoc-4.6
    update-alternatives --quiet --remove native2ascii /usr/bin/gnative2ascii-4.6
    update-alternatives --quiet --remove rmic /usr/bin/grmic-4.6
    update-alternatives --quiet --remove serialver /usr/bin/gserialver-4.6
    update-alternatives --quiet --remove tnameserv /usr/bin/gtnameserv-4.6
fi


