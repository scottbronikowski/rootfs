#!/bin/sh

set -e

basedir=/usr/lib/jvm/java-6-openjdk-armhf
base7dir=/usr/lib/jvm/java-7-openjdk-armhf
tools='javaws'

if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ]; then
    for i in $tools; do
        update-alternatives --remove $i $basedir/jre/bin/$i
    done
    for i in $tools; do
        update-alternatives --remove $i $base7dir/jre/bin/$i
    done
fi


