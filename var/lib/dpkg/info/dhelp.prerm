#!/bin/sh -e



rm -rf /var/lib/dhelp
rm -rf /usr/doc/HTML /usr/share/doc/HTML
rm -f /var/lib/dhelp/configured

if [ \( "$1" = "upgrade" -o "$1" = "remove" \) \
     -a -L /usr/doc/dhelp ]; then
  rm -f /usr/doc/dhelp
fi


if [ \( "$1" = "upgrade" -o "$1" = "remove" \) \
     -a -L /usr/doc/HTML ]; then
  rm -f /usr/doc/HTML
fi
