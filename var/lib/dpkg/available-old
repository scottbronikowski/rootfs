Depends: libasound2 (>= 1.0.24.1), libc6 (>= 2.15-0ubuntu8), libdbus-1-3 (>= 1.1.1), libgcc1 (>= 1:4.4.0), libltdl7 (>= 2.4.2), liborc-0.4-0 (>= 1:0.4.16), libpulse0 (>= 1:1.0), libsamplerate0 (>= 0.1.7), libsndfile1 (>= 1.0.20), libspeexdsp1 (>= 1.2~beta3.2-1), libtdb1 (>= 1.2.7+git20101214), libudev0 (>= 147), libx11-6, libx11-xcb1, upstart-job, adduser, lsb-base (>= 3), consolekit, libasound2-plugins, pulseaudio-utils
