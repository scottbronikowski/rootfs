        <script type="text/javascript">
             function ContinueMO(){
                var elem = document.getElementById("connection");
                if( elem.selectedIndex  &&  document.getElementById("enhanced-app").checked===true )
                        document.getElementById("landingContinue").title = "You will be connected for this session only";//alert("You will be connected for this session only");                
                else
                {
                  
                if(  elem.selectedIndex === 1 )
                    document.getElementById("landingContinue").title = "You will be connected for every session";//alert("You will be connected for every session");
              
                }
            }
            
            function EnhInstallSel(){ 
            
               document.getElementById("si1a").style.visibility="visible";
               document.getElementById("si1a").style.color="Red";
               document.getElementById("si1b").style.visibility="hidden";
                document.getElementById("basic-app").title="";
            } 
            function BasicInsSel(){ 
               document.getElementById("si1b").style.visibility="visible";
               document.getElementById("si1b").style.color="Red";        
               document.getElementById("si1a").style.visibility="hidden";
                    
            } 
            function  OnContinue() {
                if (window && window.landingUI && window.landingUI.state)
                {
                    window.landingUI.state.onContinue();
                    
                    //window.landingUI.state = 
                }
            }
            var timer_on = false;
            var time = 30;
           function timedCount() {
                  if( timer_on===false )
                  {
                   timer_on = true;
                        document.getElementById("SAVEDTimer").innerHTML = time;
                        setTimeout("timedCount()",1000); 
                  }       
                  else
                  {
                  elem =  document.getElementById("SAVEDTimer");
                    document.getElementById("SAVEDTimer").innerHTML = time;
                    time=time-1;
                    if( time>= 0 )
                        setTimeout("timedCount()",1000); 
                    else
                        OnCloseSettingsSaved();  
                  
                  }      
            }
            function  OnCloseSettingsSaved() {
                         //$("#landingSAVED").hide();
                         //$('.mainwindow').show();
                if (window && window.landingUI && window.landingUI.state)
                {
                    window.landingUI.state.onClose();
                }
            }
            function  OnCloseDeviceRemoved() {
                if (window && window.landingUI && window.landingUI.state)
                {
                    window.landingUI.state.onClose();
                }
            }
            
            function OnDownloadEnhancedInstall() {
             if (window && window.landingUI && window.landingUI.state)
                {
                    window.landingUI.state.onContinue();
                }
            
                
            }
            
            function OnLandMTLM( elem )
            {
               document.getElementById("LandMTLM").style.visibility="visible";
               document.getElementById("LandMTLM").style.top="185px";
               document.getElementById("LandMTLM").style.left="405px";
               document.getElementById("LandMTLM").show();
                         window.setTimeout(function(){
                          document.getElementById("LandMTLM").style.visibility="hidden";
                        }, 10000);
                                                          
            }
            
            function OnAutoConLM()
            {
               document.getElementById("AutoConLM").style.visibility="visible";
               document.getElementById("AutoConLM").style.top="337px";
               document.getElementById("AutoConLM").style.left="22px";
               document.getElementById("AutoConLM").show();
                         window.setTimeout(function(){
                          document.getElementById("AutoConLM").style.visibility="hidden";
                        }, 10000);
            
            }

            function AutoconnectSel() 
            {
                var elem = document.getElementById("connection");
                if (elem.selectedIndex) 
                {
                    var msg = "When auto connect is enabled you will be automatically connected and remain connected when a Verizon network is available.\r\rVerizon Mobile Broadband Manager is not required to connect to the Verizon network, and the connection will continue if the Verizon Mobile Broadband Manager is closed.\r\rClick \"Ok\" to accept or \"Cancel\" to reject.";
                    if( elem.selectedIndex==1 )
                        msg = "When auto connect is enabled you will be automatically connected and remain connected when a Verizon network is available.\r\rVerizon Mobile Broadband Manager is not required to connect to the Verizon network, and the connection will continue if the Verizon Mobile Broadband Manager is closed.\r\rThe charge for data transmitted and received while roaming can be as high as $20.48 per MB. Please check your price plan for the applicable charges in the location where you plan to access the service. To check your usage, please go to the &quot;My Verizon&quot; button on the Verizon Mobile Broadband Manager. Data usage charges will apply for accessing this link in accordance with your price plan.\r\rClick \"Ok\" to accept or \"Cancel\" to reject.";                      
                    
                      var choice = confirm(msg);

                      if (!choice)
                          elem.selectedIndex = 0;
                }
                
                if (elem.selectedIndex)
                {
                    document.getElementById("autoconmsg").style.visibility="visible";
                    document.getElementById("autoconmsg").style.top="-10px";
                    document.getElementById("autoconmsg").style.left="40px";
                }
                else
                {
                    document.getElementById("autoconmsg").style.visibility="hidden";
                }
     
            }
               
            function EULAAccepted()
            {
                if(   window && window.landing && window.landingUI )
                {
                     if ( window.landingUI.state)
                        window.landingUI.state.onContinue();
                }
            }
            
            function OnMouseOverBasicInstall()
            {
                var elem = document.getElementById("basic-app");
                if (elem.checked) 
                {
                 document.getElementById("si1blabel").title="You will not see an option to disconnect in the Basic App client UI unless you disconnect your VPN first.";    
                }    
                else    
                document.getElementById("si1blabel").title=""; 
            }
            
            function IPPassThruInstallSel()
            {
            }
            
            function onSaveBookmarkPrompt()
            {
                if (jQuery.browser.msie) {
                             
                                    alert("Right-click on a blank portion of the page and click Add to Favorites.\n\nor\n 1.Move to the page you wish to add to your Favorites.\n 2.At the top of the browser window click Favorites.\n 3.Click Add to Favorites.\n");
                            }
                            else if (jQuery.browser.safari) {
                                    alert("Click the + icon in the address bar, or press Ctrl + D, then click Add");
                     
                            }
                            else if (jQuery.browser.chrome || jQuery.browser.webkit) {
                                    alert("Click the star icon in the address bar, or press Ctrl + D, then click Done");
                            }
                            else if (jQuery.browser.mozilla) {
                                        alert("Right-click on a blank portion of the page you wish to bookmark and click Bookmark This Page or press Ctrl + D.\n\nor\n1.Move to the page you wish to bookmark.\n2.At the top of the browser window click Bookmarks\n3.Click Bookmark This Page");
                            }
                            else if (jQuery.browser.opera) {
                                       alert("Right-click on a blank portion of the page you wish to bookmark and click Bookmark This Page or press Ctrl + T.\n\nor\n1.Move to the page you wish to bookmark.\n2.At the top of the browser window click Bookmarks\n3.Click Bookmark Page");
                            }
            }
                        
