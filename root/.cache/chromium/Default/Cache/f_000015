        <!--
        //Just in case any of the images don't get served up correctly.  Let's
        //just automatically re-request them (up to 10 times).
        function onImgError(img)
        {
            window.setTimeout(function()
                            {
                                var osrc = img.getAttribute('osrc'),
                                    ncount = img.getAttribute('ncount');
                                    
                                if (!osrc)
                                {
                                    osrc = img.getAttribute('src');
                                    img.setAttribute('osrc', osrc);
                                }   
                                
                                if (!ncount)
                                    ncount = '1';
                                    
                                img.setAttribute('ncount', ++ncount);
                                
                                if (ncount <= 10)
                                {
                                    var d = new Date();
                                    img.src = osrc + '?c=' + d.valueOf();
                                }
                            }, 
                            1000);
        }
        
        -->
        </script>
        
